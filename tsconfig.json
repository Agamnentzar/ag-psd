{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": ["es2016", "dom"],
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveConstEnums": true,
		"declaration": true,
		"useUnknownInCatchVariables": false,
		"strict": true
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}