{
  "name": "ag-psd",
  "version": "11.0.0",
  "description": "Library for reading and writing PSD files",
  "main": "dist/index.js",
  "module": "dist-es/index.js",
  "browser": "dist/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Agamnentzar/ag-psd"
  },
  "scripts": {
    "test": "gulp test",
    "test2": "mocha -R dot dist/test",
    "dev": "gulp dev --tests",
    "lint": "tslint -c tslint.json src/ts/**/*.ts",
    "build": "gulp build && browserify dist/index.js --standalone agPsd -o dist/bundle.js",
    "prepublish": "npm run build"
  },
  "jspm": {
    "main": "dist/index.js",
    "jspmNodeConversion": false,
    "dependencies": {
      "base64-js": "^1.3.0"
    }
  },
  "keywords": [
    "psd",
    "photoshop"
  ],
  "author": "Agamnentzar",
  "license": "MIT",
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "browserify": "^16.5.0",
    "canvas": "^2.6.1",
    "chai": "^4.2.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-spawn-mocha": "^6.0.0",
    "gulp-typescript": "^5.0.1",
    "merge2": "^1.3.0",
    "mkdirp": "^1.0.3",
    "mocha": "^7.0.1",
    "remap-istanbul": "^0.13.0",
    "source-map-support": "^0.5.16",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "yargs": "^15.1.0"
  },
  "dependencies": {
    "@types/base64-js": "^1.2.5",
    "base64-js": "^1.3.1",
    "mkdir": "0.0.2",
    "parse-engine-data": "^0.1.2"
  }
}
