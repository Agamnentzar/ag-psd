{
  "name": "ag-psd",
  "version": "6.0.0",
  "description": "Library for reading and writing PSD files",
  "main": "dist/index.js",
  "module": "dist-es/index.js",
  "browser": "dist/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Agamnentzar/ag-psd"
  },
  "scripts": {
    "test": "gulp test",
    "dev": "gulp dev --tests",
    "lint": "tslint -c tslint.json src/ts/**/*.ts",
    "build": "gulp build && browserify dist/index.js --standalone agPsd -o dist/bundle.js",
    "prepublish": "npm run build"
  },
  "jspm": {
    "main": "dist/index.js",
    "jspmNodeConversion": false,
    "dependencies": {
      "base64-js": "^1.3.0"
    }
  },
  "keywords": [
    "psd"
  ],
  "author": "Agamnentzar",
  "license": "MIT",
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.1",
    "browserify": "^16.2.3",
    "canvas": "^2.3.1",
    "chai": "^4.2.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-spawn-mocha": "^5.0.1",
    "gulp-typescript": "^5.0.1",
    "merge2": "^1.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^6.0.2",
    "remap-istanbul": "^0.13.0",
    "source-map-support": "^0.5.11",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333",
    "yargs": "^13.2.2"
  },
  "dependencies": {
    "@types/base64-js": "^1.2.5",
    "base64-js": "^1.3.0"
  }
}
