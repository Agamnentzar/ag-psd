{
  "name": "ag-psd",
  "version": "14.3.7",
  "description": "Library for reading and writing PSD files",
  "main": "dist/index.js",
  "module": "dist-es/index.js",
  "es2015": "dist-es/index.js",
  "browser": "dist/index.js",
  "sideEffects": true,
  "repository": "git@github.com:Moonpig/ag-psd.git",
  "scripts": {
    "test": "gulp test",
    "test2": "mocha -R dot dist/test",
    "dev": "gulp dev --tests",
    "lint": "tslint -c tslint.json src/ts/**/*.ts",
    "build": "gulp build && browserify dist/index.js --standalone agPsd -o dist/bundle.js",
    "rollup": "rollup --format=cjs --external base64-js --file=dist-es/bundle.js --sourcemap --sourcemapFile dist-es/bundle.js.map dist-es/index.js",
    "prepublishOnly": "npm run build",
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "jspm": {
    "main": "dist/index.js",
    "jspmNodeConversion": false,
    "dependencies": {
      "base64-js": "^1.3.1"
    }
  },
  "keywords": [
    "psd",
    "photoshop"
  ],
  "author": "Moonpig",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^7.0.4",
    "@semantic-release/npm": "^8.0.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.0",
    "browserify": "^17.0.0",
    "canvas": "^2.8.0",
    "chai": "^4.3.4",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-spawn-mocha": "^6.0.0",
    "gulp-typescript": "^5.0.1",
    "merge2": "^1.4.1",
    "mocha": "^8.4.0",
    "remap-istanbul": "^0.13.0",
    "semantic-release": "^18.0.1",
    "source-map-support": "^0.5.19",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/base64-js": "^1.3.0",
    "base64-js": "^1.5.1"
  },
  "bugs": {
    "url": "https://github.com/Moonpig/ag-psd/issues"
  },
  "homepage": "https://github.com/Moonpig/ag-psd#readme",
  "directories": {
    "test": "test"
  }
}
